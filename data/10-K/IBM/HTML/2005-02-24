<!-- ZEQ.=1,SEQ=20,EFW="2152314",CP="IBM CORPORATION",DN="1",CHK=36270,FOLIO='19',FILE='DISK053:[05NYC3.05NYC2533]JC2533A.;11',USER='MKEANE',CD='23-FEB-2005;11:50' -->
