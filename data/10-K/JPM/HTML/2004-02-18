    <TD>&nbsp;</TD>
    <TD>&nbsp;</TD>
    <TD align="left">$</TD>
    <TD align="right">2,751</TD>
    <TD>&nbsp;</TD>
    <TD>&nbsp;</TD>
    <TD align="left"><B>$</B></TD>
