    <TD nowrap align="left">Year ended December 31,<SUP>(a)</SUP></TD>
    <TD>&nbsp;</TD>
    <TD nowrap align="right" colspan="2">&nbsp;</TD>
    <TD>&nbsp;</TD>
    <TD>&nbsp;</TD>
    <TD nowrap align="right" colspan="2">&nbsp;</TD>
    <TD>&nbsp;</TD>
    <TD>&nbsp;</TD>
    <TD nowrap align="right" colspan="2">&nbsp;</TD>
    <TD>&nbsp;</TD>
