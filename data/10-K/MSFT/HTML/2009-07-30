<TD VALIGN="bottom" ALIGN="right"><FONT FACE="Arial" SIZE="2">1,543</FONT></TD>
<TD NOWRAP VALIGN="bottom"><FONT FACE="Arial" SIZE="2">&nbsp;&nbsp;</FONT></TD>
<TD VALIGN="bottom"><FONT SIZE="1">&nbsp;&nbsp;</FONT></TD>
<TD VALIGN="bottom"><FONT FACE="Arial" SIZE="2">$</FONT></TD>
<TD VALIGN="bottom" ALIGN="right"><FONT FACE="Arial" SIZE="2">1,663</FONT></TD>
<TD NOWRAP VALIGN="bottom"><FONT FACE="Arial" SIZE="2">&nbsp;&nbsp;</FONT></TD>
<TD VALIGN="bottom"><FONT SIZE="1">&nbsp;&nbsp;</FONT></TD>
