<!-- Begin Table Body -->
<TR style="font-size: 1px">
    <TD colspan="5" align="left" style="border-top: 1px solid #0070ff">&nbsp;</TD>
    <TD align="left" style="border-right: 1px solid #0070ff; border-top: 1px solid #0070ff">&nbsp;</TD>
    <TD align="left" style="border-top: 1px solid #0070ff">&nbsp;</TD>
    <TD colspan="7" align="left" style="border-top: 1px solid #0070ff">&nbsp;</TD>
</TR>
<TR valign="bottom">
    <TD><DIV style="margin-left:10px; text-indent:-10px">Taxes on income<SUP>1</SUP></DIV></TD>
    <TD>&nbsp;</TD>
    <TD>&nbsp;</TD>
