    <TD width="5%">&nbsp;</TD>
    <TD width="1%">&nbsp;</TD><!-- VRule -->
    <TD width="1%">&nbsp;</TD>
    <TD width="1%">&nbsp;</TD>
</TR>

<!-- End Table Head -->
<!-- Begin Table Body -->
<TR valign="bottom" style="color: #0077C0; background: #E1EAF7; font-size: 9pt">
    <TD valign="top" align="left"><DIV style="margin-left:15px; text-indent:-0px; margin-top: 15px">Supplemental Information
on Oil and Gas Producing Activities <BR>&nbsp;<BR>&nbsp;</DIV></TD>
    <TD>&nbsp;</TD>
    <TD>&nbsp;</TD>
    <TD>&nbsp;</TD>
    <TD align="left" valign="top">&nbsp;</TD>
    <TD>&nbsp;</TD>
    <TD>&nbsp;</TD>
