<TR valign="bottom">
    <TD><DIV style="margin-left:10px; text-indent:-10px"><FONT size="2">Memo: Special gains</FONT></DIV></TD>
    <TD><FONT size="2">&nbsp;</FONT></TD>
    <TD align="right"><FONT size="2">&nbsp;</FONT></TD>
    <TD align="right"><FONT size="2"><B>604</B></FONT></TD>
    <TD><FONT size="2">&nbsp;</FONT></TD>
    <TD><FONT size="2">&nbsp;</FONT></TD>
    <TD><FONT size="2">&nbsp;</FONT></TD>
    <TD align="right"><FONT size="2">1,193</FONT></TD>
    <TD><FONT size="2">&nbsp;</FONT></TD>
    <TD><FONT size="2">&nbsp;</FONT></TD>
    <TD><FONT size="2">&nbsp;</FONT></TD>
    <TD align="right"><FONT size="2">451</FONT></TD>
    <TD><FONT size="2">&nbsp;</FONT></TD>
