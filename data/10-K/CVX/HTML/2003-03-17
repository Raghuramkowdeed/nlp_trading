    <TD width="2%">&nbsp;</TD>
    <TD width="1%">&nbsp;</TD>
    <TD width="2%">&nbsp;</TD>
    <TD width="5%">&nbsp;</TD>
    <TD width="2%">&nbsp;</TD>
    <TD width="1%">&nbsp;</TD>
    <TD width="2%">&nbsp;</TD>
    <TD width="5%">&nbsp;</TD>
    <TD width="2%">&nbsp;</TD>
    <TD width="1%">&nbsp;</TD>
    <TD width="2%">&nbsp;</TD>
</TR>
<TR valign="bottom">
    <TD nowrap align="left"><FONT size="1">Millions of dollars</FONT></TD>
    <TD><FONT size="1">&nbsp;</FONT></TD>
    <TD nowrap align="center" colspan="3"><FONT size="1"><B>2002</B></FONT></TD>
