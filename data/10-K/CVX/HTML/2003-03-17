    <TD><FONT size="2">&nbsp;</FONT></TD>
    <TD align="right"><FONT size="2">$</FONT></TD>
    <TD align="right"><FONT size="2">4,360</FONT></TD>
    <TD><FONT size="2">&nbsp;</FONT></TD>
    <TD><FONT size="2">&nbsp;</FONT></TD>
    <TD align="right"><FONT size="2">$</FONT></TD>
    <TD align="right"><FONT size="2">6,322</FONT></TD>
    <TD><FONT size="2">&nbsp;</FONT></TD>
