    <TD colspan="5"><DIV style="margin-left:10px; text-indent:-10px"><B>Total Special Items</B></DIV></TD>
    <TD>&nbsp;</TD>
    <TD nowrap align="left"><B>$</B></TD>
    <TD align="right"><B>(53</B></TD>
    <TD nowrap><B>)</B></TD>
    <TD>&nbsp;</TD>
    <TD nowrap align="left">$</TD>
    <TD align="right">(3,334</TD>
    <TD nowrap>)</TD>
    <TD>&nbsp;</TD>
