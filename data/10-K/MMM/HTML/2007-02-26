  <td width="6%" colspan="2" valign="bottom" style="padding:0pt .7pt 0pt 0pt;width:6.72%;">
  <p style="margin:0pt 0pt .0001pt;page-break-after:avoid;text-align:right;"><font size="2" face="Times New Roman" style="font-size:10.0pt;">2,333</font></p>
  </td>
  <td width="2%" valign="bottom" style="padding:0pt .7pt 0pt 0pt;width:2.38%;">
  <p style="margin:0pt 0pt .0001pt;page-break-after:avoid;"><font size="2" face="Times New Roman" style="font-size:1.0pt;">&nbsp;</font></p>
  </td>
  <td width="6%" valign="bottom" style="padding:0pt .7pt 0pt 0pt;width:6.56%;">
  <p style="margin:0pt 0pt .0001pt;page-break-after:avoid;text-align:right;"><font size="2" face="Times New Roman" style="font-size:10.0pt;">11.0</font></p>
  </td>
  <td width="3%" valign="bottom" style="padding:0pt .7pt 0pt 0pt;width:3.56%;">
  <p style="margin:0pt 0pt .0001pt;page-break-after:avoid;"><font size="2" face="Times New Roman" style="font-size:10.0pt;">%</font></p>
  </td>
  <td width="6%" colspan="2" valign="bottom" style="padding:0pt .7pt 0pt 0pt;width:6.56%;">
  <p style="margin:0pt 0pt .0001pt;page-break-after:avoid;text-align:right;"><font size="2" face="Times New Roman" style="font-size:10.0pt;">447</font></p>
