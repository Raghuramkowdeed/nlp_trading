    <TD><FONT size="2">&nbsp;</FONT></TD>
    <TD align="right" valign="bottom"><FONT size="2">&nbsp;</FONT></TD>
    <TD align="right" valign="bottom" nowrap><B><FONT size="2">2,089</FONT></B></TD>
    <TD><FONT size="2">&nbsp;</FONT></TD>
    <TD><FONT size="2">&nbsp;</FONT></TD>
    <TD align="right" valign="bottom"><FONT size="2">&nbsp;</FONT></TD>
    <TD align="right" valign="bottom" nowrap><FONT size="2">2,106</FONT></TD>
    <TD><FONT size="2">&nbsp;</FONT></TD>
    <TD><FONT size="2">&nbsp;</FONT></TD>
