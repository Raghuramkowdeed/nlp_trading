<TD VALIGN="bottom"><FONT FACE="Times New Roman" SIZE="2"><B>&nbsp;&nbsp;$</B></FONT></TD>
<TD VALIGN="bottom" ALIGN="right"><FONT FACE="Times New Roman" SIZE="2"><B>3,714&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</B></FONT></TD>
<TD VALIGN="bottom"><FONT SIZE="1">&nbsp;&nbsp;&nbsp;&nbsp;</FONT></TD>
<TD VALIGN="bottom"><FONT FACE="Times New Roman" SIZE="2">&nbsp;&nbsp;$</FONT></TD>
<TD VALIGN="bottom" ALIGN="right"><FONT FACE="Times New Roman" SIZE="2">4,182&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</FONT></TD>
<TD VALIGN="bottom"><FONT SIZE="1">&nbsp;&nbsp;&nbsp;&nbsp;</FONT></TD>
<TD VALIGN="bottom"></TD>
<TD VALIGN="bottom"></TD></TR>
<TR STYLE="font-size:1px">
<TD VALIGN="bottom"></TD>
<TD VALIGN="bottom">&nbsp;&nbsp;&nbsp;&nbsp;</TD>
<TD VALIGN="bottom" STYLE="border-top:3px double #000000">&nbsp;</TD>
