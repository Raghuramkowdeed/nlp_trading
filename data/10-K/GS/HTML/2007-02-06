    <TD colspan="3" align="left" valign="top">
    &nbsp;&nbsp;<FONT face="helvetica,arial">Basic
    </FONT></TD>
    <TD>&nbsp;</TD>
    <TD align="right" valign="bottom"><FONT face="helvetica,arial">$</FONT></TD>
    <TD align="right" valign="bottom" nowrap><FONT face="helvetica,arial">11.64</FONT></TD>
    <TD>&nbsp;</TD>
    <TD>&nbsp;</TD>
    <TD align="right" valign="bottom"><FONT face="helvetica,arial">$</FONT></TD>
    <TD align="right" valign="bottom" nowrap><FONT face="helvetica,arial">8.98</FONT></TD>
    <TD>&nbsp;</TD>
