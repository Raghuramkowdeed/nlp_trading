<!-- ZEQ.=1,SEQ=2,EFW="2104280",CP="INTEL CORPORATION",DN="1",CHK=773839,FOLIO='i',FILE='DISK013:[03PAL8.03PAL1008]BG1008A.;10',USER='DLAQUIA',CD=';8-MAR-2003;04:18' -->
