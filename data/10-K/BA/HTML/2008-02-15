<TD VALIGN="bottom" WIDTH="4%"></TD>
<TD></TD>
<TD></TD>
<TD VALIGN="bottom" WIDTH="4%"></TD>
<TD></TD>
<TD></TD></TR>
<TR>
<TD VALIGN="bottom" STYLE="BORDER-BOTTOM:1px solid #000000">&nbsp;<FONT SIZE="1">&nbsp;</FONT></TD>
<TD VALIGN="bottom" STYLE="BORDER-BOTTOM:1px solid #000000"><FONT SIZE="1">&nbsp;&nbsp;</FONT></TD>
<TD VALIGN="bottom" COLSPAN="5" ALIGN="center" STYLE="BORDER-BOTTOM:1px solid #000000"><FONT FACE="ARIAL" SIZE="2"><B>2007</B></FONT></TD>
<TD VALIGN="bottom" STYLE="BORDER-BOTTOM:1px solid #000000"><FONT SIZE="1">&nbsp;&nbsp;</FONT></TD>
<TD VALIGN="bottom" COLSPAN="5" ALIGN="center" STYLE="BORDER-BOTTOM:1px solid #000000"><FONT FACE="ARIAL" SIZE="2">2006</FONT></TD></TR>
<TR>
<TD VALIGN="bottom" STYLE="BORDER-BOTTOM:1px solid #000000">&nbsp;<FONT SIZE="1">&nbsp;</FONT></TD>
<TD VALIGN="bottom" STYLE="BORDER-BOTTOM:1px solid #000000"><FONT SIZE="1">&nbsp;&nbsp;</FONT></TD>
<TD VALIGN="bottom" COLSPAN="2" ALIGN="right" STYLE="BORDER-BOTTOM:1px solid #000000"><FONT FACE="ARIAL" SIZE="2"><B>Carrying<BR>Amount</B></FONT></TD>
<TD VALIGN="bottom" STYLE="BORDER-BOTTOM:1px solid #000000"><FONT SIZE="1">&nbsp;&nbsp;</FONT></TD>
<TD VALIGN="bottom" COLSPAN="2" ALIGN="right" STYLE="BORDER-BOTTOM:1px solid #000000"><FONT FACE="ARIAL" SIZE="2"><B>Fair<BR>Value</B></FONT></TD>
<TD VALIGN="bottom" STYLE="BORDER-BOTTOM:1px solid #000000"><FONT SIZE="1">&nbsp;&nbsp;</FONT></TD>
<TD VALIGN="bottom" COLSPAN="2" ALIGN="right" STYLE="BORDER-BOTTOM:1px solid #000000"><FONT FACE="ARIAL" SIZE="2">Carrying<BR>Amount</FONT></TD>
<TD VALIGN="bottom" STYLE="BORDER-BOTTOM:1px solid #000000"><FONT SIZE="1">&nbsp;&nbsp;</FONT></TD>
