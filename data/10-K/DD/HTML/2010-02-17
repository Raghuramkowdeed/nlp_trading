<!-- ZEQ.=2,SEQ=109,EFW="2196441",CP="E.I. DU PONT DE NEMOURS AND CO.",DN="1",CHK=288746,FOLIO='F-49',FILE='DISK105:[10ZAB1.10ZAB74601]KS74601A.;7',USER='JLAWRENA',CD='17-FEB-2010;13:04' -->
