<TABLE WIDTH="100%" BORDER=0 CELLSPACING=0 CELLPADDING=0>
<TR VALIGN="BOTTOM">
<TH COLSPAN=10 ALIGN="CENTER"><HR NOSHADE></TH>
<TH WIDTH="2%"><FONT SIZE=1>&nbsp;</FONT></TH>
</TR>
<TR VALIGN="BOTTOM">
<TH WIDTH="49%" ALIGN="LEFT"><FONT SIZE=1><I>Pretax (Dollars in millions)</I></FONT><BR></TH>
<TH WIDTH="4%"><FONT SIZE=1>&nbsp;</FONT></TH>
<TH COLSPAN=2 ALIGN="CENTER"><FONT SIZE=1><B>2004</B></FONT><BR></TH>
<TH WIDTH="4%"><FONT SIZE=1>&nbsp;</FONT></TH>
<TH COLSPAN=2 ALIGN="CENTER"><FONT SIZE=1>2003</FONT><BR></TH>
<TH WIDTH="4%"><FONT SIZE=1>&nbsp;</FONT></TH>
