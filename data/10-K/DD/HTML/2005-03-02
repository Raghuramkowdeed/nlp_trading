<!-- ZEQ.=2,SEQ=14,EFW="2151039",CP="E. I. DU PONT DE NEMOURS",DN="1",CHK=966298,FOLIO='14',FILE='DISK020:[05PHI6.05PHI1036]DE1036A.;88',USER='AMCMICH',CD='23-FEB-2005;17:59' -->
