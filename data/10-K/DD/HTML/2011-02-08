<!-- ZEQ.=2,SEQ=100,EFW="2201761",CP="E.I. DU PONT DE NEMOURS AND CO.",DN="1",CHK=313675,FOLIO='F-41',FILE='DISK122:[11ZAE1.11ZAE49201]KY49201A.;63',USER='DSLOAN',CD=';4-FEB-2011;11:59' -->
